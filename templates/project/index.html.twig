{% extends 'base.html.twig' %}

{% block title %}Liste des projets{% endblock %}

{% block body %}
    <div class="container">
        <h1>Liste des projets :</h1>
        <div class="row">
            <article class="col-4">
                <div class="card">
                    {% for project in projects  %}
                        {% include 'project/_thumbnail.html.twig' with {'project': project} %}
                    {% endfor %}
                </div>
            </article>
        </div>
    </div>

    <a href="{{ path('project_new') }}">Create new</a>
{% endblock %}